{
  "name": "ecommittra",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "set NODE_ENV=development && node start-production.mjs",
    "dev:frontend": "vite",
    "dev:backend": "tsx watch backend/index.ts",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cross-env NODE_OPTIONS=--max-old-space-size=2048 npm run build:frontend:internal",
    "build:frontend:internal": "tsc && vite build",
    "build:backend": "cross-env NODE_OPTIONS=--max-old-space-size=2048 tsc --project tsconfig.backend.json",
    "start": "cross-env NODE_ENV=production node start-production.mjs",
    "start:dev": "tsx backend/index.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "verify": "node verify-setup.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "postinstall": "echo 'Skipping auto-build on install. Run npm run build manually after setting up environment.'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.916.0",
    "@aws-sdk/s3-request-presigner": "^3.916.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-window": "^1.8.8",
    "argon2": "^0.31.2",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.0",
    "framer-motion": "^11.0.25",
    "helmet": "^7.2.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.368.0",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.15.0",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-query": "^3.39.3",
    "react-window": "^2.2.0",
    "react-window-infinite-loader": "^2.0.0",
    "recharts": "^3.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "wouter": "^3.1.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/jspdf": "^1.3.3",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.14.9",
    "@types/pdfkit": "^0.13.5",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.38",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.3",
    "terser": "^5.31.0",
    "tsx": "^4.16.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^4.0.13"
  }
}
